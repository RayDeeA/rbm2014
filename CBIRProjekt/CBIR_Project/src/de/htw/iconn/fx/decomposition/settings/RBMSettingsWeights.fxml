<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" fx:id="view" prefHeight="-1.0" prefWidth="-1.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="de.htw.iconn.fx.decomposition.settings.RBMSettingsWeightsController">
  <children>
    <GridPane hgap="10.0" layoutX="0.0" layoutY="0.0" vgap="10.0">
      <children>
        <Button mnemonicParsing="false" onAction="#btn_setWeightsRandomAction" prefWidth="135.0" text="Random" GridPane.columnIndex="0" GridPane.rowIndex="3" />
        <Button mnemonicParsing="false" onAction="#btn_loadWeightsAction" prefWidth="124.0" text="Load" GridPane.columnIndex="1" GridPane.rowIndex="3" />
        <Button mnemonicParsing="false" onAction="#btn_saveCurrentWeightsAction" prefWidth="105.0" text="Save Current" GridPane.columnIndex="2" GridPane.rowIndex="3" />
        <CheckBox fx:id="cbx_useSeed" mnemonicParsing="false" onAction="#cbx_useSeedAction" text="Seed" GridPane.columnIndex="0" GridPane.rowIndex="2" />
        <TextField fx:id="txt_seed" onKeyReleased="#txt_seedKey" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
        <CheckBox fx:id="cbx_useBinarizeHidden" mnemonicParsing="false" onAction="#cbx_useBinarizeHiddenAction" text="Binarize Hidden" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.rowIndex="0" />
        <CheckBox fx:id="cbx_useBinarizeVisible" mnemonicParsing="false" onAction="#cbx_useBinarizeVisibleAction" text="Binarize Visible" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.rowIndex="1" />
      </children>
      <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      </columnConstraints>
      <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
      </padding>
      <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      </rowConstraints>
    </GridPane>
  </children>
</AnchorPane>
